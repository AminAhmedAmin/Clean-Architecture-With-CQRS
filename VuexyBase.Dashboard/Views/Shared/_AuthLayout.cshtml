    <script src="~/assets/vendor/libs/i18n/i18n.js"></script>
    <script src="~/assets/vendor/libs/typeahead-js/typeahead.js"></script>
    <script src="~/assets/vendor/js/menu.js"></script>
    <script src="~/assets/vendor/libs/toastr/toastr.dist.js"></script>
    <script src="~/assets/vendor/libs/toastr/toastr.js"></script>
    <script src="~/assets/js/ui-toasts.dist.js"></script>
    <script src="~/assets/js/ui-toasts.js"></script>
    <!-- endbuild -->
    <!-- Main JS -->
    <script src="~/assets/js/main.js"></script>

    <!-- Page JS -->
    <script src="~/assets/js/pages-auth.js"></script>

    <!-- endbuild -->
    <!-- Vendors JS -->
    <script src="~/assets/vendor/libs/formvalidation/umd/bundle/popular.min.js"></script>
    <script src="~/assets/vendor/libs/formvalidation/umd/plugin-bootstrap5/index.min.js"></script>
    <script src="~/assets/vendor/libs/formvalidation/umd/plugin-auto-focus/index.min.js"></script>


    <!-- / Content -->
    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->

    <!-- Main JS -->
    <script src="~/assets/js/main.js"></script>

    <!-- Page JS -->
    <script src="~/assets/js/pages-auth.js"></script>
    @await RenderSectionAsync("VendorScripts", required: false)

    <!-- Main JS -->
    <script src="~/assets/js/main.js"></script>

    <!-- Page JS -->
    @await RenderSectionAsync("PageScripts", required: false)
</body>
</html>
