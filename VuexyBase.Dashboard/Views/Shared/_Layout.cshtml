    <script>

        //hide canva after submit
        document.body.addEventListener("htmx:afterOnLoad", function (e) {
            const trigger = e.detail.xhr.getResponseHeader("HX-Trigger");

            if (trigger?.includes("toggleOffcanvas")) {
                const offcanvasEl = document.getElementById('offcanvasAddData');
                const offcanvasEl2 = document.getElementById('offcanvasEditData');

                const offcanvas = bootstrap.Offcanvas.getOrCreateInstance(offcanvasEl);
                const offcanvas2 = bootstrap.Offcanvas.getOrCreateInstance(offcanvasEl2);


                if (offcanvasEl.classList.contains('show')) {
                    offcanvas.hide();
                    let formInputs = document.querySelectorAll('#createform .add-input');
                    console.log(formInputs);
                    console.log("triggered")
                    for(let i=0; formInputs.length > i ;i++){
                           formInputs[i].value = "";
                    }
                }

                if (offcanvasEl2.classList.contains('show')) {
                    offcanvas2.hide();
                }
            }
        });

    </script>



