@using VuexyBase.Application.Common.Helpers
@using VuexyBase.Domain.Entities.Identities
@{
    string userId = UserManager.GetUserId(User);
    var user = new ApplicationDbUser();

    // var lang = requestCulture?.RequestCulture.UICulture.TwoLetterISOLanguageName;

    if (userId is not null)
    {
        user = await UserManager.FindByIdAsync(userId);
    }

    var feature = Context.Features.Get<IRequestCultureFeature>();
    var currentCulture = feature?.RequestCulture.UICulture.TwoLetterISOLanguageName ?? "en";
    TempData["language"] = @currentCulture;
    var dir = currentCulture == "ar" ? "rtl" : "ltr";

}

